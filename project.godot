[application]

config/name="Underwater Co-Op"
run/main_scene="res://Logic/Main.tscn"
config/icon="res://icon.png"

[autoload]

Nakama="*res://addons/com.heroiclabs.nakama/Nakama.gd"
OnlineMatch="*res://addons/nakama-webrtc/OnlineMatch.gd"
Game="*res://Logic/Game.tscn"
Network="*res://Networking/Network.tscn"
Dialog="*res://Logic/Dialog.tscn"

[debug]

gdscript/warnings/unused_variable=false
gdscript/warnings/unused_argument=false
gdscript/warnings/unused_signal=false
gdscript/warnings/return_value_discarded=false
multirun/number_of_windows=2
multirun/window_distance=200
multirun/add_custom_args=true
multirun/first_window_args="debug player1"
multirun/other_window_args="debug"

[display]

window/vsync/use_vsync=false

[editor_plugins]

enabled=PoolStringArray( "res://addons/multirun/plugin.cfg" )

[gdnative]

singletons=[ "res://Networking/webrtc/webrtc.gdnlib", "res://Networking/webrtc_debug/webrtc_debug.gdnlib" ]

[gui]

common/drop_mouse_on_gui_input_disabled=true
theme/custom="res://UI/Styles/theme.tres"

